{
  "expo": {
    "name": "Viralpool",
    "slug": "viralpool-content-creator-mobile-app",
    "description": "Viralpool mobile application for content creators.",
    "privacy": "public",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo_square.png",
    "splash": {
      "image": "./assets/logo_square.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tharindarajapakshe.viralpoolapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo_square.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.tharindarajapakshe.viralpoolapp",
      "useNextNotificationsApi": true
    },
    "web": {
      "favicon": "./assets/logo_square.png"
    }
  }
}
